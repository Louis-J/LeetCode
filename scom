#include <type_traits>

namespace GTL::Switch {

template <class TypeC, class TypeD>
struct Case {
    TypeC key;
    TypeD deal;
};

} // namespace GTL::Switch

#endif // GTL_SWITCH_SWITCHCOMMON_HPP
